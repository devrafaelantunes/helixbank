<h1>New Account</h1>

<%= form_for @changeset, Routes.user_path(@conn, :create), fn f -> %>
    <div>
        <%= text_input f, :name, placeholder: "Name" %>
    </div>
    
    <div>   
        <%= text_input f, :document, placeholder: "Document" %>
    </div>

    <%= submit "Create Account" %>
<% end %>